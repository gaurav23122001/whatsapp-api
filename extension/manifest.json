{
	"version": "1.0.2",
	"manifest_version": 3,
	"name": "WhatsLeads",
	"description": "Enhance WhatsApp Web: Blur, Export & Send Message. Elevate privacy, message ending and contact management effortlessly!",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": [
		"tabs",
		"storage",
		"scripting"
	],
	"host_permissions": [
		"https://web.whatsapp.com/*"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "index.html",
		"default_title": "React Chrome Extension"
	}
}